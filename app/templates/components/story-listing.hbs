<article class="story">
  <div class="story__left">
    <a class="story__vote">&#9650;</a>
  </div>
  <div class="story__right">
    <h3 class="story__title"><a href="{{story.url}}" target="_blank">{{story.title}}</a></h3>
    <ul class="story__info">
      <li class="story__info__owner">{{story.owner}}</li>
    </ul>
  </div>
</article>
